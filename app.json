{
  "expo": {
    "name": "liquid-app",
    "slug": "liquid-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "liquidapp",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.liquid-app",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "liquidapp",
              "com.googleusercontent.apps.612466735730-3a257kmveufsc6f476ais9djm5t36irg"
            ]
          }
        ],
        "NSLocationWhenInUseUsageDescription": "Cette application utilise votre localisation pour vous fournir des informations météo personnalisées et des recommandations vestimentaires adaptées à votre région.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Cette application utilise votre localisation pour vous fournir des informations météo personnalisées et des recommandations vestimentaires adaptées à votre région."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.liquidapp",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
